{
  "name": "backend",
  "description": "backend",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 16.19.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "sqlite",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "cross-env NODE_ENV=test npm run migrate && npm run mocha",
    "test:watch": "npm run test -- -- --require ts-node/register --watch --watch-files src, **/*.test.ts",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public",
    "migrate": "knex migrate:latest",
    "migrate:down": "knex migrate:down",
    "migrate:make": "knex migrate:make",
    "migrate:status": "knex migrate:status",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "like-prod": "./scripts/like-prod.sh",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.30",
    "@feathersjs/authentication": "^5.0.30",
    "@feathersjs/authentication-client": "^5.0.30",
    "@feathersjs/authentication-local": "^5.0.30",
    "@feathersjs/authentication-oauth": "^5.0.30",
    "@feathersjs/configuration": "^5.0.30",
    "@feathersjs/errors": "^5.0.30",
    "@feathersjs/feathers": "^5.0.30",
    "@feathersjs/knex": "^5.0.30",
    "@feathersjs/koa": "^5.0.30",
    "@feathersjs/schema": "^5.0.30",
    "@feathersjs/socketio": "^5.0.30",
    "@feathersjs/transport-commons": "^5.0.30",
    "@feathersjs/typebox": "^5.0.30",
    "@json2csv/plainjs": "^6.1.3",
    "@koa/multer": "^3.0.2",
    "chai": "^4.5.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "feathers-hooks-common": "^7.0.3",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "postmark": "^3.11.0",
    "sharp-multer": "^0.3.3",
    "sinon": "^15.2.0",
    "sqlite3": "^5.1.7",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.30",
    "@feathersjs/rest-client": "^5.0.30",
    "@types/koa__multer": "^2.0.7",
    "@types/mocha": "^10.0.9",
    "@types/node": "^18.19.55",
    "@types/sinon": "^10.0.20",
    "axios": "^1.7.7",
    "cross-env": "^7.0.3",
    "mocha": "^10.7.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2"
  }
}
